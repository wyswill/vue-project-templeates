    "grpc": "^1.24.10",
    "log4js": "^6.3.0",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
