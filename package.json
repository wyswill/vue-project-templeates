  "scripts": {
    "prebuild": "rimraf twitter-server",
    "build": "nest build",
    "release": "npm-run-all build release:cp release:tgz release:dep",
    "release:cp": "cross-env-shell \"cp package.json .env  docker/* $npm_package_name/ \"  ",
    "release:tgz": "tar -zcvf $npm_package_name.tgz $npm_package_name",
    "release:dep": "scp $npm_package_name.tgz weita:/root",
    "start": "nest start",
    "dev": "nest start --watch"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.7",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/swagger": "^4.7.15",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.2.0",
    "helmet": "^4.4.1",
    "lodash": "^4.17.20",
    "log4j": "^1.0.0",
    "moment": "^2.29.1",
    "mongo": "^0.1.0",
    "nestjs-redis": "^1.2.8",
    "nodemailer": "^6.4.18",
    "qiniu": "^7.3.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "token": "^0.1.0",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/autocannon": "^4.1.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/log4js": "^2.3.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.0",
    "@types/qiniu": "^7.0.1",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/twitter": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "autocannon": "^7.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/domain",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@libs/lib/(.*)": "<rootDir>/libs/lib/src/$1",
      "@libs/lib": "<rootDir>/libs/lib/src"
    }
  }
}